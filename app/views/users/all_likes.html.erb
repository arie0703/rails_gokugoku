<div class ="allview-wrapper">
  <div class ="allview-btn">
    <%= link_to "戻る", :back %>
  </div>
  <% @user.liked_posts.each do |t| %>
    <div class ="postview">
      <p><%= t.product%></p>
      <h4><%= t.title%></h4>              
      <p>投稿日時: <%= t.created_at.to_s(:datetime_jp)%></p>
      <%= link_to "", post_path(t.id) %>
    </div>
  <% end %>
</div>